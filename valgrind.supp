{
   <AssertFailureLeaksTuvx>
   Memcheck:Leak
   match-leak-kinds: definite
   fun:malloc
   ...
   fun:InternalCreateTuvx
   ...
}
{
  <AssertFailureLeaksString>
  Memcheck:Leak
  match-leak-kinds: definite
  ...
  fun:CreateTuvx
  ...
}
{
  <AssertFailureLeaksTuvxConstructor>
  Memcheck:Leak
  match-leak-kinds: definite
  fun:malloc
  fun:*musica_tuvx*
  ...
}